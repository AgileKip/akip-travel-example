      <bpmn:incoming>Flow_0d290kr</bpmn:incoming>
      <bpmn:outgoing>Flow_0g6w5kp</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:businessRuleTask id="Activity_0h260ug" name="Calculate Day" camunda:resultVariable="result" camunda:decisionRef="calculate_day" camunda:mapDecisionResult="singleEntry">
      <bpmn:extensionElements />
