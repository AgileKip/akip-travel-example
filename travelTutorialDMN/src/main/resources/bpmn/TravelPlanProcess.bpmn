    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_0zzu1gn" sourceRef="Gateway_0xwfpx9" targetRef="TaskCar">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${result == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
